root = true

[*]
charset = utf-8
end_of_line = crlf
indent_size = 4
indent_style = space
insert_final_newline = false
max_line_length = 120
tab_width = 4

[*.scss]
indent_size = 4

[{*.ats,*.ts}]
indent_size = 4

[{*.css,*.wxss}]
indent_size = 4

[{*.ng,*.sht,*.html,*.shtm,*.shtml,*.htm}]
tab_width = 4

[{*.wxs,*.js,*.cjs}]
tab_width = 4
